<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="spawning" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="spawning" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="biome">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="spawn" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:attribute name="maxcount" type="xs:string" />
                  <xs:attribute name="respawndelay" type="xs:string" />
                  <xs:attribute name="time" type="xs:string" />
                  <xs:attribute name="entitygroup" type="xs:string" />
                  <xs:attribute name="spawnDeadChance" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="name" type="xs:string" />
          </xs:complexType>
        </xs:element>
        <xs:element name="entityspawner">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="day" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="property" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="name" type="xs:string" />
                        <xs:attribute name="value" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="value" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="name" type="xs:string" />
            <xs:attribute name="dynamic" type="xs:string" />
            <xs:attribute name="wrapMode" type="xs:string" />
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>